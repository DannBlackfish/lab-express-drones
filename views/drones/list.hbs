<h2>Available drones</h2>
{{#each this.allDrones}}
<p>{{this.name}}</p>
<p>{{this.propellers}}</p>
<p>{{this.speed}}</p>
<a href="/drones/{{id}}/edit">Editar</a>
<form action="/drones/{{id}}/delete" method="POST">
    <button type="submit">Borrar</button>
</form>
{{/each }}

<a href="/drones/create">Add drone</a>